{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "ecmaFeatures" : {
    "modules": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": false } ],
    "complexity": [2, 10],
    "strict": 2,
    "max-len": [2, 120, 4],
    "no-use-before-define": [2, "nofunc"],
    "quotes": [2, "single", "avoid-escape"],
    "no-var": 2,
    "yoda": 0,
    "curly": [2, "multi-line"],
    "new-cap": 0,
    "eqeqeq": 2
  }
}
